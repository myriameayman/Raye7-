label {
    float:left;
    width:180px;
}

input, 
select, 
textarea, 
button {
    background:#fff;
    font-size:1em;
    font-family:"HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-weight:300;
    color:#8c8a8b;
